{
  "repository": {
        "statements": {
            "patterns": [
                {
                    "match": "(\\b)(?<=.*(dump|delete_key|insert|insert_or_update|modify|delete|clear))(\\s+)([A-Z][a-zA-Z0-9_]*)(\\s*)",
                    "name": "entity.name.relation.ddlog"
                },                                                                                       
                {
                    "name": "string.text.dat",
                    "match": "\\b(?<=echo)(\\s+)(.*)?$"
                },
                {
                    "name": "comment.hash.dat ",
                    "begin": "#",
                    "end": "$"
                }                  
            ]
        }
    }
}

