      "required": true,
      "schema": { "type": "string" }
    },
    {
      "name": "sort",
      "description": "Sorts the results of your query by `author-date` or `committer-date`. Default: [best match](https://developer.github.com/enterprise/2.20/v3/search/#ranking-search-results)",
