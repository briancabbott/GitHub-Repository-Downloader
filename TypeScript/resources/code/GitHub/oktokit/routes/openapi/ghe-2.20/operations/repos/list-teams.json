    {
      "lang": "JS",
      "source": "octokit.repos.listTeams({\n  owner: 'octocat',\n  repo: 'hello-world'\n})"
    },
    { "lang": "Ruby", "source": "octokit.teams(\n  'hello-world'\n)" }
  ],
