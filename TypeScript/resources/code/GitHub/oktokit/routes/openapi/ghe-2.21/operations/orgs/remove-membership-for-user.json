      "name": "accept",
      "description": "Setting to `application/vnd.github.v3+json` is recommended",
      "in": "header",
      "schema": {
        "type": "string",
        "default": "application/vnd.github.v3+json"
      }
    },
    {
      "name": "org",
      "in": "path",
      "required": true,
      "schema": { "type": "string" }
    },
    {
      "name": "username",
      "in": "path",
      "required": true,
      "schema": { "type": "string" }
    }
  ],
  "responses": { "204": { "description": "Empty response" } },
  "x-code-samples": [
