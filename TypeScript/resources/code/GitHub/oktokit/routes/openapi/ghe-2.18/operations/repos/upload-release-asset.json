      "schema": { "type": "string" }
    },
    {
      "name": "release_id",
      "description": "release_id parameter",
      "in": "path",
      "required": true,
      "schema": { "type": "integer" }
    },
    {
      "name": "name",
      "in": "query",
      "schema": { "type": "string" },
      "description": "name parameter"
    },
    {
      "name": "label",
      "in": "query",
      "schema": { "type": "string" },
      "description": "label parameter"
    }
  ],
