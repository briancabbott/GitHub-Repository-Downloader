init_config:

instances:

    ## @param url - string - required
    ## Base URL to your Aqua instance.
    #
  - url: http://your-aqua-instance.com

    ## @param api_user - string - required
    ## Username the Agent should use to query the Aqua API.
    #
    # api_user: <USERNAME>

    ## @param password - string - required
    ## Password associated with the `api_user` username to query the Aqua API.
    #
    # password: <PASSWORD>

    ## @param tags - list of key:value elements - optional
    ## List of tags to attach to every metric, event and service check emitted by this integration.
    ##
    ## Learn more about tagging: https://docs.datadoghq.com/tagging/
    #
    # tags:
    #   - <KEY_1>:<VALUE_1>
    #   - <KEY_2>:<VALUE_2>
