[
    {
        "agent_version": "6.0.0",
        "integration": "Nextcloud",
        "check": "nextcloud.can_connect",
        "statuses": [
            "ok",
            "critical"
        ],
        "groups": [
            "host"
        ],
        "name": "Can Connect",
        "description": "Returns `CRITICAL` if the Agent can't connect to the Nextcloud endpoint, OK otherwise"
    }
]
