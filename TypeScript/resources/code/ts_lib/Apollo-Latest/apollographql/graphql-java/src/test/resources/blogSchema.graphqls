schema {
    query: Query
    mutation: Mutation
}

type Query {
    posts: [Post]
    author(id: Int!): Author
}

type Mutation {
    upvotePost (
    postId: Int!
): Post
}


type Author {
    id: Int!
    firstName: String
    lastName: String
    posts: [Post]
}

type Post {
    id: Int!
    title: String
    votes: Int
    author: Author
}
